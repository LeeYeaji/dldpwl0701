.google_map{position:relative; width:80%; height:300px; margin:50px auto }
/*대중교통*/
.md_transportation{width:1200px;height:270px;border:1px solid #ffffff;position:relative;}
.md_transportation > p{margin-left:150px;margin-top:50px;}
.md_transportation > p > a > .md_span1{position:absolute;top:65px; left:200px;}
.md_transportation > p > a > .md_span2{position:absolute;top:170px; left:200px;}
.md_transportation > p > a > .md_spanimg{position:absolute;left:240px;}
.md_transportation2{width:716px;height:99px;border:3px solid #f2f2f2;position:absolute;top:25px; left:300px;}
.md_transportation2 > p{line-height:35px}
.md_transportation3{width:716px;height:76px;border:3px solid #f2f2f2;position:absolute;top:140px; left:300px;}
.md_transportation3 > p{line-height:76px}
/*주차안내 */
.md_parking{width:1200px;height:270px;position:relative;}
.md_parking h3{position:absolute;top:15px;left:50px}
.md_parkinginfo{position:absolute;left:250px;}
/*관람료*/
.md_moviemoney{width:1200px;border:1px solid #ffffff;height:600px;margin-top:100px;position:relative;}
.md_moviemoney h3{position:absolute;top:15px;left:50px}
.md_moviemoney table{width:900px;height:500px;margin:0 auto;border-collapse:collapse}
.md_moviemoney table th{border:1px solid black; background:#f7fbfe;}
.md_moviemoney table tr td{text-align:center;border:1px solid black;}
.md_moviemoney > table > caption{visibility:hidden;}
