/*movie.html-------------------------------------------*/

/*공통스타일*/
/*중간 공통*/
#check_movie > li{position:relative;overflow:hidden;border:3px solid #0d92d7;width:291px;float:left;margin-left:2px;}
#check_movie > li.sel_focus{position:relative;border:3px solid #0d92d7;top:-2px;}
#check_movie > li > h3{padding:10px 0;text-align:center;background:#2f2f2f;color:#fff001}
#step1_jump a,#step2_jump a,#step3_jump a,#step4_jump a{position:absolute;top:0;left:-9999px;z-index:100}
#step1_jump a:focus,#step1_jump a:active,#step2_jump a:focus,#step2_jump a:active,#step3_jump a:focus,#step3_jump a:active,#step4_jump a:focus,#step4_jump a:active{left:0;width:100%;background-color:#0d92d7;color:#ffffff;padding:10px 0;font-weight:bold;text-align:center}
#check_movie > li > #check_order,
#check_movie > li > #check_date,
#check_movie > li > #check_order2,
#check_movie > li > #check_time{font-size:0.95em;border-bottom:2px solid #000000;padding:7px 0 9px 5px;background:#e7e7e7}

#re_movie_list li,
#date_list li,
#seater_list > dl > dt,
#seater_list > dl > dd > ul > li{height:36px;line-height:36px;border-bottom:1px solid #979797;cursor:pointer;transition:.5s ease}
#re_movie_list li.choice{background:#2f2f2f;}
#re_movie_list li.choice a.a_choice{color:#fff001;}
/*#re_movie_list li:last-child,
#date_list li:last-child,
#seater_list > dl > dt:last-child,
#seater_list > dl > dd > ul > li:last-child{border-bottom:1px}*/

#check_order > a,
#check_order2 > a{padding:2px 5px;border:1px solid #979797;text-align:center;font-size:0.8em;border-radius:3px;}

#check_order > a:first-child{margin-left:5px}
#check_order > a img,#check_order2 > a img{padding-left:3px}

/*상단*/
#re_section{position:relative;width:1200px;padding-bottom:150px;margin:0 auto;}
#re_section > h2{padding:30px 0 120px 0;font-size:1.5em;}
#re_section > h2 > span{font-size:0.7em;font-weight:normal;padding-left:10px}
#re_section > #re_btn{position:absolute;top:30px;right:10px;background:#0d92d7;color:#ffffff;border:1px solid #585858;padding:8px 55px;}
/*중간*/
#check_movie > li{height:500px}
#check_movie > li:nth-child(3){height:auto}
#check_movie > li:first-child{margin-left:0}
#check_movie > li > #check_date{padding-left:15px}
#check_order > a.order,#check_order2 > a.order{border:1px solid #111111;background:#0d92d7;color:#ffffff}
#re_movie_list > li > a{position:relative;color:#000000;padding-left:15px;bottom:5px;font-size:0.85em}
#re_movie_list > li > a > img{position:relative;top:5px;right:5px;padding-top:8px}
#check_month > h4{padding:8px 0 0 15px;background:#595959;color:#fff001}
#check_month > ul.order{padding:8px 0 0 15px;width:100%;background:#595959;height:30px}
#check_month > ul.order > li{float:left;}
#check_month > ul.order > li:first-child{margin-right:5px}
#check_month > ul.order > li > a{padding:5px 8px;border:1px solid #ffffff;color:#ffffff}
#check_month > ul.order > li > a.set_month{border:1px solid #0d92d7;background:#000000;color:#fff001}
#date_list{clear:both}
#date_list > li > a{font-size:0.85em;color:#000000;padding-left:15px}
#date_list > li.on{background:#2f2f2f}
#date_list > li > a.on_text{color:#fff001}
#seater_list > dl > dt{font-size:0.85em;background:#0072bc;text-align:center}
#seater_list > dl > dt > a{color:#ffffff}
#seater_list > dl > dd{font-size:0.85em}
#seater_list > dl > dd > ul > li > a{padding-left:15px;color:#000000}
#seater_list > dl > dd > ul > li.seat_on{background:#2f2f2f}
#seater_list > dl > dd > ul > li > a.seat_text{color:#fff001}
#check_movie > li > #check_time{padding-left:15px;padding-top:7px}
#detail_time > li > h4{float:left;padding:15px 20px 0 15px;height:100%}
#detail_time > li > ul{width:80%;height:95px;padding:10px 0 0 65px;border-bottom:1px solid #707070;}
#detail_time > li > ul:last-child{border-bottom:1px solid #707070}
#detail_time > li > h4{clear:both}
#detail_time > li > ul > li > a{margin:3px;padding:1px 5px;border:1px solid #707070;background:#e1e1e1;float:left;font-size:0.85em;color:#000000;font-weight:bold}
#detail_time > li > ul > li > a.sel_time{background:#2f2f2f;color:#fff001}
#customer_check{position:absolute;width:100%;top:75px}
#customer_check h3{font-size:0px;line-height:0px;position:absolute;top:0px;left:-9999px;text-indent:-9999px}
#customer_check #sel_contents{width:100%;margin:0 auto;}
#customer_check #sel_contents li{position:relative;float:left;width:24.7%;background:#1d1d1c;border:1px solid #b4b4b4;padding:0 0 20px 0;text-align:center;margin-bottom:10px}
#customer_check #sel_contents li:first-child{border-radius:5px 0 0 5px;}
#customer_check #sel_contents li:last-child{border-radius:0 5px 5px 0}
#customer_check #sel_contents li h4{color:#cccccc;font-family:"나눔바른고딕";padding-top:18px;font-weight:bold}
#customer_check #sel_contents li p{padding-top:10px;color:#3399ff;font-weight:bold}
#customer_check #sel_contents li p.click_focus{color:#3399ff;font-weight:bold}

/*movie_seat.html-------------------------------------------*/
#people_select{position:relative;width:100%;margin:0 auto}
#people_select h3{padding:5px 30px 5px 10px;font-weight:normal;font-family:"나눔바른고딕";background:#3399ff;color:#ffffff;font-size:1em;width:260px}
#seat_skip a{position:absolute;top:0;left:-9999px;z-index:100}
#seat_skip a:focus{box-shadow:0;left:0;width:260px;background-color:#0d92d7;color:#ffffff;padding:4px 31px 6px 9px;text-align:center}
#check_people{position:relative;width:100%;height:72px;border:1px solid #979797;}
#check_people h4{padding:25px 15px 15px;float:left;}
#check_people ul{float:left}
#check_people ul li{position:relative;float:left;width:258px;height:70px;margin-right:20px}
#check_people ul li h5{position:absolute;left:20px;top:28px}
#check_people ul li p{position:absolute;font-size:0.8em;top:5px;right:30px}
#check_people ul li .people{position:absolute;top:35px;font-weight:bold}
#check_people ul li .num_control .num_left_Btn{position:absolute;top:28px;left:70px}
#check_people ul li .num_control .num_right_Btn{position:absolute;top:28px;right:50px}
#check_people ul li #adult_num,
#check_people ul li #youth_num,
#check_people ul li #vip_num{position:absolute;top:33px;right:115px;color:#000000;font-size:1em;font-weight:bold}
#price_check{position:absolute;top:0;right:0;}
#price_check h5{padding:5px;position:absolute;top:0;right:150px;width:30px;font-size:1.0em}
#price_check p{position:absolute;width:37px;top:3px;right:0px}
#price_check_num{position:absolute;width:150px;top:25px;right:10px;font-weight:bold;font-size:2em;color:#0d92d7;text-align:right}
#seat_explain{position:relative;width:100%;height:34px;border:1px solid #979797;border-top:0;}
#seat_explain h3{position:absolute;font-size:0;top:0;left:-9999px}
#seat_explain p{position:absolute;top:2px;left:10px}
#seat_explain p:last-child{left:110px}
#seat_explain p span{position:absolute;width:60px;top:6px;left:35px}
#seat_sel_click{float:left;margin-bottom:150px;position:relative;width:816px;height:559px;background:#5a5a5a;border:1px solid #000000}
#seat_sel_click h3{position:absolute;font-size:0;top:0;left:-9999px}
#seat_sel_click .seat_screen{position:relative;margin:30px auto 30px auto;font-size:1.3em;font-weight:bold;width:80%;text-align:center;background:#00a0e9;color:white;border:1px solid #000000}
#seat_sel_click_num{width:80%;margin:0 auto}
#seat_sel_click .seat_entryway{position:absolute;left:30px;bottom:175px;font-family:"나눔바른고딕";padding:10px 10px;border:1px solid #000000;background:#ffffff;color:#000000}
#seat_sel_click .seat_exit{position:absolute;right:30px;bottom:175px;font-family:"나눔바른고딕";padding:10px 10px;border:1px solid #000000;background:#ffffff;color:#000000}
#reserve_total_layer{position:relative;color:#ffffff;font-size:0.85em;background:#404040;overflow:hidden;left:2px;width:365px;float:right;border:1px solid #000000;border-top:0}
#reserve_total_layer h3{width:100%;padding:10px 0px 10px 0px;background:#3399ff;color:#ffffff;text-align:center}
#reserve_total_layer table{width:100%;border-collapse:collapse;font-family:"나눔바른고딕"}
#reserve_total_layer table caption{position:absolute;font-size:0;top:0;left:-9999px}
#reserve_total_layer th,#reserve_total_layer td{border:1px solid #000000;padding:12px 0;text-align:center}
#reserve_total_layer td{background:#ffffff;color:#000000}
#reserve_total_layer th{width:100px;border-left:0;font-weight:bold}
#empty_control{position:relative;top:4px;right:6px}
#seat_sel_click_num li a{display:block;margin:2px;color:#000000;float:left;font-size:0.85em;width:23px;height:23px;background:#ffffff;border:1px solid #000000;line-height:25px;text-align:center;}
#seat_sel_click_num li a.alphabet{color:#ffffff;background:#1d1d1c;font-weight:bold;cursor:default}
#seat_sel_click_num li a.auto_margin{margin-left:50px}
#seat_sel_click_num li a.auto_margin2{margin-bottom:30px}
#seat_sel_click_num li a.click_seat{background:#00a0e9;color:#ffffff}
#reserve_pay a{display:block;font-family:"나눔바른고딕";font-weight:bold;font-size:12pt;width:100%;padding:9px 0;background:#3399ff;color:#ffffff;text-align:center}
