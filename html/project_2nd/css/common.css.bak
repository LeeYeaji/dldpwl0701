@charset "utf-8";
/************* Common *************/
@font-face {
    font-family: '나눔고딕';
    src: url(../font/NanumBarunGothic.ttf) format('truetype');
	src:url(../font/NanumBarunGothic.eot) format('embedded-opentype');
    src:local('※'), url(../font/NanumBarunGothic.woff) format('woff');
}
@font-face {
    font-family: '나눔바른고딕';
    src: url(../font/NanumBarunGothicBold.ttf) format('truetype');
	src:url(../font/NanumBarunGothicBold.eot) format('embedded-opentype'); 
    src:local('※'), url(../font/NanumBarunGothicBold.woff) format('woff');
}
@font-face {
    font-family: 'CALIST';
    src: url(../font/CALIST.ttf) format('truetype');
	src:url(../font/calist.eot); format('embedded-opentype');
    src:local('※'), url(../font/calist.woff) format('woff');
}


/*공통스타일*/
	/*초기화작업*/
body,h1,h2,h3,h4,h5,ul,ol,p,hr,dl,dt,dd,table,tr,td,th,caption{margin:0;padding:0}
body{font-family:"돋움",dotum,sans-serif;font-size:14px;color:#000000;line-height:1.5;-webkit-text-size-adjust:none}
h1,h2,h3,h4{font-size:100%}
ul,ol{list-style:none}
a{text-decoration:none}
a:link,a:visited,a:hover,a:active{color:#666}
img,video{border:0px;max-height:100%;height:auto}
address{font-style:normal}
header,nav,section,article,footer{display:block}

/* 2운용의 용이성 > 4쉬운네비게이션 > 1반복내용건너뛰기 : 접근성을 지키기 위한 필수 항목*/
#skip a{position:absolute;top:0;left:-9999px;z-index:9999}
#skip a:focus, #skip a:active{left:0;width:100%;background-color:#0d92d7;color:#ffffff;padding:10px 0;font-weight:bold;text-align:center}

/*공통클래스*/
.f_l{float:left}
.p_r{position:relative}
.p_a{position:absolute}
.hide{font-size:0px;line-height:0px;position:absolute;top:0px;left:-9999px;text-indent:-9999px}
.clear:after{content:"";display:block;clear:both}
.mr4_no{margin-right:0 !important}/*특별성 숫자와 상관없이 최우선 적용*/
.area{width:80%;margin:0 auto;}
#empty_zone{width:100%;height:235px}

/*상단배너광고*/
#topBanner{position:relative;width:100%;padding:0;height:80px;background:#000000;text-align:center}
#topBanner .cancel{position:absolute;top:0;right:0}
#topBanner .area p{width:970px;height:80px;margin:0 auto}
/*header*/
#header{width:100%;height:235px;position:fixed;z-index:1000}
#header #topBanner .area{height:156px;}
#header .logo{width:298px;height:55px;margin:0 auto;padding:25px 0}
#header .logo a{display:block;height:100%}
/*gnb*/
#header>.area{width:100%}
#wrap{width:100%;position:relative;background:#ffffff;font-family:"나눔바른고딕";}
#wrap h1{width:298px;height:55px;margin:0 auto;padding:22px 0}
#wrap h1.hidden{position:absolute;left:-9999px;top:0;}
#nav{width:100%;height:50px;background:#303030;border-top:6px solid #b7e4f1}
#nav > #gnb{width:1100px;height:50px;margin:0 auto;position:relative;}
#nav > #gnb > li{float:left;width:150px;height:50px;line-height:50px;text-align:center}
#nav > #gnb > li > a{color:white}
#nav > #gnb > li > ul{display:none;position:absolute;width:120px;padding-top:20px}
#nav > #gnb > li > ul > li{width:150px;padding:0px 0px;}
#nav > #gnb > li > ul > li > a{display:block;color:white}
#nav > #gnb > li > ul > li > a:hover{color:white;background:#0d92d7;border-radius:10px;}
#nav > #gnb > li > ul.on{background:#0d92d7}
#nav > #gnb > li.sub1 > ul{left:0px}
#nav > #gnb > li.sub2 > ul{left:150px}
#nav > #gnb > li.sub3 > ul{left:300px}
#nav > #gnb > li.sub4 > ul{left:450px}
#nav > #gnb > li.sub5 > ul{left:600px}
#nav > #gnb > li.sub6 > ul{left:750px}
#nav > #gnb > li.sub7 > ul{left:900px}
#panel{display:none;position:absolute;left:0;width:100%;height:290px;z-index:-100;background:#000000;background:rgba(0,0,0,.85);/*filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#000000,endColorstr=#000000);*/}



